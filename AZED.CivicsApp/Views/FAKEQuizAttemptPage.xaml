<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             x:Class="AZED.CivicsApp.Views.FAKEQuizAttemptPage">
    <ContentPage.Content>

      <StackLayout Padding="10" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">

            <Label Text="Section" />
            <Entry Text="{Binding SectionName}" />

            
            <Label Text="QuestionID" />
            <Entry Text="{Binding QuestionID_1}" />
            <Label Text="GivenAnswer" />
            <Entry Text="{Binding GivenAnswer_1}" />
            <Label Text="CorrectYN" />
            <Entry Text="{Binding CorrectYN_1}" />

            <Label Text="QuestionID" />
            <Entry Text="{Binding QuestionID_2}" />
            <Label Text="GivenAnswer" />
            <Entry Text="{Binding GivenAnswer_2}" />
            <Label Text="CorrectYN" />
            <Entry Text="{Binding CorrectYN_2}" />


            <Button Text="Save" Clicked="Save_Clicked" />

            <Button Text="Delete" Clicked="Delete_Clicked" />
            
    </StackLayout>
    </ContentPage.Content>
</ContentPage>
